{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "abordage/laravel-html-min": "^0.2.0",
        "abordage/laravel-og-images": "^0.4.1",
        "ausi/slug-generator": "^1.1",
        "bavix/laravel-wallet": "^10.1",
        "browner12/helpers": "^3.5",
        "camroncade/timezone": "^0.2.0",
        "darryldecode/cart": "^4.2",
        "directorytree/bartender": "^1.0",
        "donatello-za/rake-php-plus": "^1.0",
        "guzzlehttp/guzzle": "^7.2",
        "jenssegers/agent": "^2.6",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/folio": "^1.1.5",
        "laravel/framework": "^10.38",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.5.0",
        "livewire/volt": "^1.6.0",
        "marksitko/laravel-unsplash": "^2.2",
        "mashape/unirest-php": "^3.0",
        "maxmind-db/reader": "^1.11",
        "oddvalue/laravel-drafts": "^1.3",
        "openai-php/client": "^0.8.4",
        "openai-php/laravel": "^0.8.1",
        "opis/closure": "^3.6",
        "orhanerday/open-ai": "^5.1",
        "oscarotero/inline-svg": "^2.0",
        "pragmarx/google2fa-laravel": "^2.3",
        "razorpay/razorpay": "^2.9",
        "sakanjo/laravel-easy-metrics": "^1.0",
        "stripe/stripe-php": "^14.8"
    },
    "require-dev": {
        "amirami/localizator": "^0.13.0@alpha",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.26",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "reliese/laravel": "^1.2",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=livewire:assets --ansi --force",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "amirami/localizator"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
