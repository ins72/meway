{
    "name": "mewayz/platform",
    "type": "project",
    "description": "The Complete Creator Economy Platform",
    "keywords": ["laravel", "framework", "creator", "economy", "bio-sites", "social-media", "ecommerce"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "guzzlehttp/guzzle": "^7.2",
        "bavix/laravel-wallet": "^9.0",
        "lab404/laravel-impersonate": "^1.7",
        "livewire/livewire": "^2.12",
        "spatie/laravel-permission": "^5.0",
        "intervention/image": "^2.7",
        "nesbot/carbon": "^2.62",
        "predis/predis": "^1.1",
        "laravel/socialite": "^5.5",
        "socialiteproviders/manager": "^4.2",
        "socialiteproviders/instagram": "^4.1",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/twitter": "^4.1",
        "socialiteproviders/google": "^4.1",
        "darryldecode/cart": "^4.2",
        "laravel/ui": "^4.0",
        "pragmarx/google2fa": "^8.0",
        "endroid/qr-code": "^4.7",
        "bacon/bacon-qr-code": "^2.0",
        "dompdf/dompdf": "^2.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "maatwebsite/excel": "^3.1",
        "openai-php/client": "^0.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}